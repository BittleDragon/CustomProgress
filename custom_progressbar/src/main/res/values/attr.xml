<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomProgressBarTypeNumber">
        <attr name="cus_progress_unreached_color" format="color" />
        <attr name="cus_progress_reached_color" format="color" />
        <attr name="cus_progress_reached_bar_height" format="dimension" />
        <attr name="cus_progress_unreached_bar_height" format="dimension" />
        <attr name="cus_progress_text_size" format="dimension" />
        <attr name="cus_progress_text_color" format="color" />
        <attr name="cus_progress_text_offset" format="dimension" />
        <attr name="cus_progress_text_visibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
        </attr>
        <attr name="cus_progressStyle" format="enum">
            <enum name="horizontal" value="2"/>
            <enum name="ring" value="3"/>
        </attr>
        <attr name="cus_radius" format="dimension" />
    </declare-styleable>
    
    
    <!--<declare-styleable name="RoundProgressBarWidthNumber">-->
        <!--<attr name="radius" format="dimension" />-->
    <!--</declare-styleable>-->
</resources>